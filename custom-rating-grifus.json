{
   "id": "CustomRatingGrifus",
   "name": "Custom Rating",
   "version": "1.0.6",
   "description": {
      "en": "Replaces IMDB rating by your own rating system.",
      "es": "Reemplaza valoración IMDB por tu propio sistema de valoración."
   },
   "state": "uninstalled",
   "category": "wp-plugin-extension",
   "url": "https://github.com/eliasis-framework/custom-rating-grifus.git",
   "author": "Josantonius",
   "author-url": "https://josantonius.com/",
   "license": "GPL-2.0+",
   "hooks-controller": "Launcher",
   "url-import": "https://raw.githubusercontent.com/eliasis-framework/custom-rating-grifus/master/",
   "installation-files": {
      "custom-rating-grifus": {
         "0": "custom-rating-grifus.json",
         "1": "LICENSE",
         "2": "composer.json",
         "config": [
            "add-urls.php",
            "assets.php",
            "menu.php",
            "namespaces.php",
            "pages.php",
            "paths.php",
            "set-hooks.php"
         ],
         "languages": [
            "extensions-for-grifus-rating-es_ES.mo",
            "extensions-for-grifus-rating-es_ES.po"
         ],
         "public": {
            "css": {
               "0": "custom-rating-grifus.min.css",
               "1": "custom-rating-grifus-admin.min.css",
               "2": "custom-rating-grifus-edit-post.min.css",
               "source": [
                  "custom-rating-grifus.css",
                  "custom-rating-grifus-admin.css",
                  "custom-rating-grifus-edit-post.css"
               ]
            },
            "images": [
               "custom-rating-grifus.png"
            ],
            "js": {
               "0": "custom-rating-grifus.min.js",
               "1": "custom-rating-grifus-admin.min.js",
               "2": "custom-rating-grifus-edit-post.min.js",
               "3": "custom-rating-grifus-home.min.js",
               "source": [
                  "custom-rating-grifus.js",
                  "custom-rating-grifus-admin.js",
                  "custom-rating-grifus-edit-post.js",
                  "custom-rating-grifus-home.js"
               ]
            }
         },
         "src": {
            "controller": {
               "0": "class-launcher.php",
               "admin": {
                  "0": "class-rating.php",
                  "page": [
                     "class-custom-rating.php"
                  ]
               }
            },
            "model": {
               "0": "class-launcher.php",
               "admin": [
                  "class-rating.php"
               ]
            },
            "template": {
               "meta-boxes": [
                  "wp-insert-post.php"
               ],
               "page": [
                  "custom-rating.php"
               ]
            }
         }
      }
   }
}
